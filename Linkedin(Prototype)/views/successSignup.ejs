<script>
		     var myLinkedinProfile = angular.module('myLinkedinProfile', []);
   			 myLinkedinProfile.controller( 'mySummaryController', function($scope,$http,$location){
  			 console.log("we are in controller linkedin");
   				$scope.onClickSummary = function(){ $http({
   				method: 'PUT',
	            url: '/CreateProfile/addSummary',
	           	data: {"summarytext":$scope.summarytext}
	           		}).success(function(response){
	           		console.log("We are in sucees of signup");
	   if(response.login == "Success")
	   				{
	           		console.log("Now in Success of insert summary");
	            	 alert("Summary updated!");
	        		
	            	}
	            else{
	            		 alert("error! All fields must be Filled");
	            	window.location = '/successSignup';
	            	}
	        }).error(function(error){
	        console.log("we are in error!!");
	            alert("error");
	        });      
	        };
	        
	        //ddddd
	       
 			}); 
 </script>
